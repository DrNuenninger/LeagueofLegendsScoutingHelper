{
  "name": "League of Legends Scouting Helper",
  "version": "1.2",
  "description": "An extention to help you scout your opponents both on premiertour as well as for Uni Liga germany.",
  "permissions": [],
  "background": {
    "scripts": []
  },
  "icons": {
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [ 
    {
   "js": ["premierTourHelperLib.js","teamPage.js"],
   "matches": [ "*://www.premiertour.gg/*/leagues/teams/*" ]
  },
  {
    "js": ["premierTourHelperLib.js","matchPage.js"],
    "matches": [ "*://www.premiertour.gg/*/leagues/matches/*" ]
   },
   {
    "js": ["premierTourHelperLib.js","toornamentMatchPage.js"],
    "matches": [ "*://www.toornament.com/*/tournaments/*/matches/*/players",
                 "*://www.toornament.com/*/tournaments/*/participants/*/info"]
   }
],
  "manifest_version": 2
}
